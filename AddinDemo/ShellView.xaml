<Window x:Class="AddinDemo.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AddinDemo"
        mc:Ignorable="d"
        Title="ShellView" Height="300" Width="300">
    <Grid>
        <ContentControl x:Name="MenuBar" Margin="0,0,0,239"></ContentControl>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" ItemsSource="{Binding Commands}"  Height="199" Margin="10,35,0,0" VerticalAlignment="Top" Width="272">

        </ListBox>
        <Button Command="{Binding TestCmd}" Content="Test" HorizontalAlignment="Left" Margin="10,240,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
